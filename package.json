{
  "name": "create-midas-front",
  "version": "1.1.0",
  "type": "module",
  "main": "build/index.js",
  "description": "Project initializer for React and Next.js apps.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Midas-Consultores/create-midas-front"
  },
  "keywords": [
    "commitlint",
    "eslint",
    "husky",
    "lint-staged",
    "next",
    "prettier",
    "react",
    "typescript"
  ],
  "license": "ISC",
  "bin": {
    "create-midas-front": "build/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "pnpm link-cli && tsup --watch",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "link-cli": "npm uninstall -g create-midas-front && npm link",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@stylistic/eslint-plugin": "2.7.2",
    "@types/node": "22.5.2",
    "@typescript-eslint/eslint-plugin": "8.8.0",
    "@typescript-eslint/parser": "8.4.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-unicorn": "55.0.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "tsup": "8.3.0",
    "typescript": "5.6.2"
  },
  "dependencies": {
    "@clack/prompts": "0.7.0",
    "@commander-js/extra-typings": "12.1.0",
    "commander": "12.1.0",
    "picocolors": "1.1.0"
  }
}
