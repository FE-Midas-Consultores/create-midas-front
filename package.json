{
  "name": "create-midas-front",
  "version": "1.2.2",
  "type": "module",
  "main": "build/index.js",
  "description": "Project initializer for React and Next.js apps.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Midas-Consultores/create-midas-front"
  },
  "keywords": [
    "commitlint",
    "eslint",
    "husky",
    "lint-staged",
    "next",
    "prettier",
    "react",
    "typescript"
  ],
  "license": "ISC",
  "bin": {
    "create-midas-front": "build/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "pnpm link-cli && tsup --watch",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "link-cli": "npm uninstall -g create-midas-front && npm link",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.6.0",
    "@stylistic/eslint-plugin": "2.12.1",
    "@types/node": "22.13.7",
    "@types/node": "22.13.0",
    "@typescript-eslint/eslint-plugin": "8.25.0",
    "@typescript-eslint/parser": "8.25.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-unicorn": "56.0.1",
    "husky": "9.1.7",
    "lint-staged": "15.4.3",
    "prettier": "3.4.2",
    "tsup": "8.3.5",
    "typescript": "5.7.3"
  },
  "dependencies": {
    "@clack/prompts": "0.8.2",
    "@commander-js/extra-typings": "13.1.0",
    "commander": "13.1.0",
    "picocolors": "1.1.1"
  }
}
